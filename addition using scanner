import java.io.*;
import java.util.Scanner;
class add{
        public static void main(String args[]){
                Scanner s = new Scanner(System.in);
                System.out.print("Enter two numbers : ");
                int a=s.nextInt();
                int b =s.nextInt();
                int c=0;
                c=a+b;
                System.out.println("Addition : "+c);
        }
}
