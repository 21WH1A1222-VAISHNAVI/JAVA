import java.io.*;
import java.util.Scanner;
class even_odd{
        public static void main(String args[]){
                Scanner s=new Scanner(System.in);
                System.out.print("Enter number : ");
                int a=s.nextInt();
                if(a%2==0)
                        System.out.println("Even");
                else System.out.println("Odd");
        }
}
